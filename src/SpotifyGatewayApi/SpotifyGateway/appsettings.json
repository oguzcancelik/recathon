{
    "AllowedHosts": "*",
    "Kestrel": {
        "EndpointDefaults": {
            "Protocols": "Http1"
        }
    },
    "GeneralOptions": {
        "Name": "SpotifyGateway",
        "IsClientAppEnabled": false
    },
    "RedisOptions": {
        "HashDb": 1,
        "QueueDb": 2,
        "LockDb": 3
    },
    "RequestOptions": {
        "RetryCount": 3
    },
    "TelegramServiceOptions": {
        "Url": "https://api.telegram.org/",
        "SendMessageAction": {
            "Path": "bot{0}/sendMessage?chat_id={1}&text={2}"
        }
    }
}
